{
  "manifest_version": 3,
  "name": "UTAWeb Assistant",
  "version": "1.0",
  "description": "Web assistant for querying website content using UTAWeb",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
} 